{
  "name": "call-tree",
  "version": "0.0.0-development",
  "description": "A library for working objects containing functions",
  "type": "module",
  "main": "source/index.js",
  "files": [
    "source"
  ],
  "scripts": {
    "format": "prettier --write '**/*.js'",
    "lint": "eslint --fix .",
    "pretest": "npm run format && npm run lint",
    "test": "ava",
    "coverage": "c8 --reporter=html --reporter=text --exclude tests npm run test",
    "develop": "ava --watch",
    "prepare": "npm run test"
  },
  "keywords": [
    "tree",
    "object",
    "concat",
    "includes",
    "map",
    "merge-with",
    "merge",
    "omit"
  ],
  "author": "Roger Berg Falk <roger@bergfa.lk> (https://bergfa.lk/)",
  "license": "MIT",
  "devDependencies": {
    "ava": "4.3.3",
    "c8": "7.11.3",
    "eslint": "8.15.0",
    "prettier": "2.6.2",
    "sinon": "14.0.0"
  },
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rogerbf/call-tree.git"
  },
  "homepage": "https://github.com/rogerbf/call-tree#readme",
  "engines": {
    "node": ">=12.20.0"
  }
}
